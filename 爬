try:
        nodes = browser.find_elements(by=By.CSS_SELECTOR, value='#positionRound .ellipsis a')
        #nodes = browser.find_elements(by=By.CSS_SELECTOR, value='.address.ellipsis span')
        print('社區：{}'.format(nodes[0].text.replace(' ', '')))   
        node_data.append(browser.find_elements(by=By.CSS_SELECTOR, value='#positionRound .ellipsis a')[1].text.replace(' ', ''))
    except IndexError:
        nodes = browser.find_elements(by=By.CSS_SELECTOR, value='.address.ellipsis span')
        print('社區：{}'.format(nodes[0].text.replace(' ', '')))   
        node_data.append(browser.find_elements(by=By.CSS_SELECTOR, value='.address.ellipsis span')[0].text.replace(' ', ''))
    except Exception as e:
        print('社區：')
        node_data.append('')
        pass
